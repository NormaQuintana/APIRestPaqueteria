<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="catalogo">
    
    <select id="obtener-roles" resultType="pojo.Catalogo.Rol">
        SELECT * FROM rol;
    </select>
    
    <select id="obtener-tipos-unidad" resultType="pojo.Catalogo.TipoUnidad">
        SELECT 
            idTipoUnidad, 
            tipoUnidad 
        FROM PacketWorld.tipoUnidad 
        ORDER BY idTipoUnidad;
    </select>

</mapper>

