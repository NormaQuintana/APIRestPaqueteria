<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="estatusEnvio">
    
    <select id="obtener-estatusEnvio" 
            resultType="pojo.Catalogo.EstatusEnvio">
        SELECT 
            idEstatusEnvio, 
            estatus AS estatusEnvio
        FROM 
            estatusEnvio;
    </select>
    
</mapper>